<VirtualHost *:80>
    DocumentRoot "/data/xhgui/xhprof_html"
    ServerName xhgui.local
    ServerAlias{% for vhost in virtual_hosts %} prof.{{vhost}}{% endfor %}

    ErrorLog "/var/log/apache2/xhgui.localhost-error.log"
    CustomLog "/var/log/apache2/xhgui.localhost-access.log" combined

    <Directory "/data/xhgui/xhprof_html">
        AllowOverride All
    </Directory>

</VirtualHost>
